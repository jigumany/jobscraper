{
    "name": "mwds/jobscraper",
    "description": "A laravel package to scrape for teaching jobs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mwds\\Jobscraper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mwds\\Jobscraper\\JobScraperServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "George Manyara",
            "email": "george@mwds.co.za"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0 || ^8.2",
        "laravel/framework": "^8.0 || ^9.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --provider=\"Mwds\\Jobscraper\\JobScraperServiceProvider\" --tag=\"config\""
        ]
    }

}
